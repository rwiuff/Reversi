<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="745.0" maxWidth="640.0" minHeight="425.0" minWidth="320.0" prefHeight="585.0" prefWidth="480.0" style="-fx-background-color: #344e41; -fx-border-color: black;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="advancedreversi.Main">
   <children>
      <Button fx:id="start" layoutX="198.0" layoutY="220.0" mnemonicParsing="false" onAction="#beginGame" prefHeight="34.0" prefWidth="83.0" text="Begin Game">
         <font>
            <Font name="Book Antiqua" size="12.0" />
         </font>
      </Button>
      <Button layoutX="198.0" layoutY="276.0" mnemonicParsing="false" onAction="#showHighScore" prefHeight="34.0" prefWidth="83.0" text="Highscore">
         <font>
            <Font name="Book Antiqua" size="12.0" />
         </font>
      </Button>
      <Button layoutX="198.0" layoutY="335.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="34.0" prefWidth="83.0" text="Exit">
         <font>
            <Font name="Book Antiqua" size="12.0" />
         </font>
      </Button>
      <ImageView blendMode="SRC_ATOP" fitHeight="150.0" fitWidth="150.0" layoutX="165.0" layoutY="52.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@icon64.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
